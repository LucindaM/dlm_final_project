var x[2,t_max], y[t_max]





model
{
  x[,1] ~ ddirch(mean_x_init[1],mean_x_init[2])
  y[1] ~ dpois( exp(x[2,1]))
  beta <- 2
  gamma <- 1.4
  for (t in 2:t_max)
  {
    x[,t] ~ ddirch(
    x[1,t-1]+ 1/6*(-beta*x[1,t-1]*x[2,t-1]),
    x[2,t-1]+ 1/6*(beta*x[1,t-1]*x[2,t-1]-gamma*x[2,t-1])
    )
    y[t] ~ dpois( exp(x[2,t]))
  }
}
